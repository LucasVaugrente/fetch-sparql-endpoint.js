{
  "name": "fetch-sparql-endpoint",
  "version": "1.0.0",
  "description": "Send a query to a SPARQL endpoint and parse the results",
  "keywords": [
    "sparql",
    "semantic web",
    "rdf",
    "rdfjs"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/fetch-sparql-endpoint.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/fetch-sparql-endpoint.js/issues"
  },
  "homepage": "https://github.com/rubensworks/fetch-sparql-endpoint.js#readme",
  "license": "MIT",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "dependencies": {
    "JSONStream": "^1.3.3",
    "is-stream": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "node-web-streams": "^0.2.2",
    "rdf-data-model": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/rdf-data-model": "^1.0.1",
    "@types/rdf-js": "^1.0.1",
    "arrayify-stream": "^1.0.0",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "pre-commit": "^1.2.2",
    "stream-to-string": "^1.1.0",
    "streamify-string": "^1.0.1",
    "ts-jest": "^21.2.3",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "mapCoverage": true
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build"
  }
}
